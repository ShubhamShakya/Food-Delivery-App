<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.MainLogin">

    <FrameLayout
        android:id="@+id/restaurant_admin_dashboard_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/restaurant_admin_dashboard_navigation_view" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/restaurant_admin_dashboard_navigation_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:menu="@menu/restaurant_admin_menu" />

</RelativeLayout>